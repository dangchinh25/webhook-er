{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "../",
    "testPathIgnorePatterns": [
        "<rootDir>/dist",
        "<rootDir>/node_modules",
        "<rootDir>/src/app"
    ],
    "testMatch": [
        "<rootDir>/src/server/tests/**/*.test.ts"
    ],
    "silent": true,
    "clearMocks": true,
    "setupFilesAfterEnv": [
        "jest-extended/all"
    ]
}